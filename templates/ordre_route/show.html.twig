            </tr>
            <tr>
                <th>Scan_or</th>
                <td>{{ ordre_route.scanOr }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ ordre_route.status ? 'Yes' : 'No' }}</td>
