{% extends 'base.html.twig' %}

{% block title %}OrdreRoute{% endblock %}

{% block body %}
    <h1>OrdreRoute</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ordre_route.id }}</td>
            </tr>
            <tr>
                <th>Num_or</th>
                <td>{{ ordre_route.numOr }}</td>
            </tr>
